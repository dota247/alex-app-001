    "autoRollbackConfiguration": {
        "enabled": true, 
        "events": [
            "DEPLOYMENT_FAILURE"
        ]
    }, 
    "deploymentStyle": {
        "deploymentType": "IN_PLACE",
        "deploymentOption": "WITHOUT_TRAFFIC_CONTROL"
    }, 
    "ec2TagSet": {
        "ec2TagSetList": [
            [
                {
                    "Type": "KEY_AND_VALUE",
                    "Value": "001-alex-statlogger-ec2",
                    "Key": "Name"
                }
            ]
        ]
    } 
}
